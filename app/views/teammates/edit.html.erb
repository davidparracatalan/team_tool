<div class="page-header">
  <h1>Editing <%=@teammate.name%>'s data</h1>
</div>
<div class="container">
  <div class="row span4">
    <%= form_for @teammate do |f| %>
    <%= render 'fields', f: f %>
    <div>
      <%= f.submit "Update", class: "btn" %>
      <%= link_to "Cancel", @teammate, class: "btn"%>
    </div>
    <% end %>
  </div>
</div>